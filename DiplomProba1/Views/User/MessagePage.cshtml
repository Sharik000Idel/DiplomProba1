
<div class="descript job_listing_area">
        <div class="job_listing_area max_width" style="padding-bottom: 80px;">
        <div class="container myflex-column content-center">
            <div class="row align-items-center">
                <div class="col-lg-6">
                    <div class="section_title">
                        <h3>Входящии</h3>
                    </div>
                </div>
                
            </div>
            <div class="job_lists max_width">
                <div class="row">
                    @if(Context.Session.GetString("CountMess").Trim() == "0"){
                        <div class="col-lg-6">
                        <div class="section_title">
                            <h3>Пусто</h3>
                        </div>
                    </div>
                    }
                    

                 @if(ViewBag.WarningDriver != null){
                        <div class="col-lg-12 col-md-12">
                        <div class="single_jobs white-bg d-flex justify-content-between">
                            <div class="jobs_left d-flex align-items-center">
                                <div class="thumb ">
                                    <img class="DriverPh" src="https://sun9-47.userapi.com/impg/5EPkkBcUFRSIeIW9a61tzwCwNspQtlHe4FecAQ/ZUVr_4GwYxw.jpg?size=69x70&quality=96&sign=bdeee8abeaaa7336f3a71e23758c35be&type=album" alt="Profile Picture">
                                </div>
                                <div class="jobs_conetent">

                                    <div class="flexwrap links_locat d-flex align-items-center">
                                        <div class="location">
                                            <h3> <i class="fa fa-map-marker"></i>Администрация сайта "Попутки"</h3>
                                        </div>
                                    </div>

                                    <h4>Вам вынесено предупреждение в связи с низким рейтингом. </h4>
                                                                    </div>
                            </div>
                            <div class="jobs_right myflex-column">
                                <div class="apply_now">
                                    @*<a class="heart_mark" href="#"> <i class="ti-heart">❤</i> </a>*@
                                       
                                        @*<a asp-controller="Route" asp-action="RoutePage" asp-route-id="@item.IdRout" class="boxed-btn3">Посмотреть</a>*@
                                     <a  class="boxed-btn3"  asp-action="ReadMess" asp-controller="User"> Прочитано</a>
                                </div>
                                @*<div class="date">
                                    <p>Дата размещениия:</p>
                                </div>*@
                            </div>
                        </div>
                 </div>
                 }
                
                 @if(ViewBag.DeleteDriver != null){
                        <div class="col-lg-12 col-md-12">
                        <div class="single_jobs white-bg d-flex justify-content-between">
                            <div class="jobs_left d-flex align-items-center">
                                <div class="thumb ">
                                    <img class="DriverPh" src="https://sun9-47.userapi.com/impg/5EPkkBcUFRSIeIW9a61tzwCwNspQtlHe4FecAQ/ZUVr_4GwYxw.jpg?size=69x70&quality=96&sign=bdeee8abeaaa7336f3a71e23758c35be&type=album" alt="Profile Picture">
                                </div>
                                <div class="jobs_conetent">

                                    <div class="flexwrap links_locat d-flex align-items-center">
                                        <div class="location">
                                            <h3> <i class="fa fa-map-marker"></i>Администрация сайта "Попутки"</h3>
                                        </div>
                                    </div>

                                    <h4>Ваш аккаунт водителя завблокирован решением сайта. </h4>
                                                                    </div>
                            </div>
                            <div class="jobs_right myflex-column">
                                <div class="apply_now">
                                    @*<a class="heart_mark" href="#"> <i class="ti-heart">❤</i> </a>*@
                                       
                                        @*<a asp-controller="Route" asp-action="RoutePage" asp-route-id="@item.IdRout" class="boxed-btn3">Посмотреть</a>*@
                                     <a  class="boxed-btn3"  asp-action="ReadMess" asp-controller="User"> Прочитано</a>
                                </div>
                                @*<div class="date">
                                    <p>Дата размещениия:</p>
                                </div>*@
                            </div>
                        </div>
                 </div>
                 }

                 @if( ViewData["TimeStartRoute"]!= null){
                      DiplomProba1.Models.Data.Route route = ViewBag.TimeStartRoute;
                        <div class="col-lg-12 col-md-12">
                                <div class="single_jobs white-bg d-flex justify-content-between">
                                    <div class="jobs_left d-flex align-items-center">
                                        <div class="thumb ">
                                            <img class="DriverPh" src="@route.IdUserNavigation.PhotoUser" alt="Profile Picture">
                                        </div>
                                        <div class="jobs_conetent">

                                            <div class="flexwrap links_locat d-flex align-items-center">
                                                <div class="location">
                                                    <h3> <i class="fa fa-map-marker"></i> Время начать поездку</h3>
                                                </div>
                                            </div>

                                            <a  asp-action="RoutePage" asp-controller="Route" asp-route-id="@route.IdRout"><h4>Маршрут: @route.BeginRoute - @route.EndRoute</h4></a>
                                                                            </div>
                                    </div>
                                    <div class="jobs_right myflex-column">
                                        <div class="apply_now">
                                             <a  class="boxed-btn3"   asp-controller="Route" asp-action="StatusRoute" asp-route-id="@route.IdRout" asp-route-status="Начать поездку"> Начать поездку</a>
                                        </div>
                                        
                                    </div>
                                </div>
                            </div>
                    }

                 @foreach (DiplomProba1.Models.Data.Userroute user in ViewBag.MessUser)
                        {
                        @if(user.StatusUserRouteId == 1){
                            <div class="col-lg-12 col-md-12">
                                <div class="single_jobs white-bg d-flex justify-content-between">
                                    <div class="jobs_left d-flex align-items-center">
                                        <div class="thumb ">
                                            <img class="DriverPh" src="@user.IdRoutNavigation.IdUserNavigation.PhotoUser" alt="Profile Picture">
                                        </div>
                                        <div class="jobs_conetent">

                                            <div class="flexwrap links_locat d-flex align-items-center">
                                                <div class="location">
                                                    <h3> <i class="fa fa-map-marker"></i>Ваша заявка на маршрут принята</h3>
                                                </div>
                                            </div>

                                            <a  asp-action="RoutePage" asp-controller="Route" asp-route-id="@user.IdRout"><h4>Маршрут: @user.IdRoutNavigation.BeginRoute - @user.IdRoutNavigation.EndRoute</h4></a>
                                                                            </div>
                                    </div>
                                    <div class="jobs_right myflex-column">
                                        <div class="apply_now">
                                             <a  class="boxed-btn3"  asp-action="ReadMessReq" asp-controller="User" asp-route-id="@user.IdUserroutes"> Прочитано</a>
                                        </div>
                                        
                                    </div>
                                </div>
                            </div>
                        }
                        @if(user.StatusUserRouteId == 2){
                            <div class="col-lg-12 col-md-12">
                                <div class="single_jobs white-bg d-flex justify-content-between">
                                    <div class="jobs_left d-flex align-items-center">
                                        <div class="thumb ">
                                            <img class="DriverPh" src="@user.IdRoutNavigation.IdUserNavigation.PhotoUser" alt="Profile Picture">
                                        </div>
                                        <div class="jobs_conetent">

                                            <div class="flexwrap links_locat d-flex align-items-center">
                                                <div class="location">
                                                    <h3> <i class="fa fa-map-marker"></i>Ваша заявка на маршрут отклонена</h3>
                                                </div>
                                            </div>

                                            <a  asp-action="RoutePage" asp-controller="Route" asp-route-id="@user.IdRout"><h4>Маршрут: @user.IdRoutNavigation.BeginRoute - @user.IdRoutNavigation.EndRoute</h4></a>
                                                                                    </div>
                                    </div>
                                    <div class="jobs_right myflex-column">
                                        <div class="apply_now">
                                            @*<a class="heart_mark" href="#"> <i class="ti-heart">❤</i> </a>*@
                                       
                                                @*<a asp-controller="Route" asp-action="RoutePage" asp-route-id="@item.IdRout" class="boxed-btn3">Посмотреть</a>*@
                                             <a  class="boxed-btn3"  asp-action="ReadMessReq" asp-controller="User"  asp-route-id="@user.IdUserroutes"> Прочитано</a>
                                        </div>
                                        @*<div class="date">
                                            <p>Дата размещениия:</p>
                                        </div>*@
                                    </div>
                                </div>
                            </div>
                        }
                        
                        }


                @foreach (DiplomProba1.Models.Data.Route item in ViewBag.VRoutes)
                    {
                        @foreach (var user in item.UsersRouteGetRoute())
                        {
                            <div class="col-lg-12 col-md-12">
                        <div class="single_jobs white-bg d-flex justify-content-between">
                            <div class="jobs_left d-flex align-items-center">
                                <div class="thumb ">
                                    <img class="DriverPh" src="@user.IdUserNavigation.PhotoUser" alt="Profile Picture">
                                </div>
                                <div class="jobs_conetent">

                                    <div class="flexwrap links_locat d-flex align-items-center">
                                        <div class="location">
                                            <h3> <i class="fa fa-map-marker"></i>Заявка на участие в маршруте</h3>
                                        </div>
                                    </div>

                                    <a  asp-action="UserPage" asp-controller="User" asp-route-id="@user.IdUser"><h4>@user.IdUserNavigation.Fulname</h4></a>
                                                                    </div>
                            </div>
                            <div class="jobs_right myflex-column">
                                <div class="apply_now">
                                    @*<a class="heart_mark" href="#"> <i class="ti-heart">❤</i> </a>*@
                                       
                                        @*<a asp-controller="Route" asp-action="RoutePage" asp-route-id="@item.IdRout" class="boxed-btn3">Посмотреть</a>*@
                                     <a  class="boxed-btn3"  asp-action="ViewRequest" asp-controller="Request">Посмотреть</a>
                                </div>
                                @*<div class="date">
                                    <p>Дата размещениия:</p>
                                </div>*@
                            </div>
                        </div>
                    </div>
                        }
                    
                    }


                @foreach (DiplomProba1.Models.Data.User user in ViewBag.RequestUser) {
                    <div class="col-lg-12 col-md-12">
                        <div class="single_jobs white-bg d-flex justify-content-between">
                            <div class="jobs_left d-flex align-items-center">
                                <div class="thumb ">
                                    <img class="DriverPh" src="@user.PhotoUser" alt="Profile Picture">
                                </div>
                                <div class="jobs_conetent">

                                    <div class="flexwrap links_locat d-flex align-items-center">
                                        <div class="location">
                                            <h3> <i class="fa fa-map-marker"></i>Оставьте отзыв на попутчика</h3>
                                        </div>
                                    </div>

                                    <a  asp-action="UserPage" asp-controller="User" asp-route-id="@user.IdUsers"><h4>@user.Fulname</h4></a>
                                                                    </div>
                            </div>
                            <div class="jobs_right myflex-column">
                                <div class="apply_now">
                                    @*<a class="heart_mark" href="#"> <i class="ti-heart">❤</i> </a>*@
                                       
                                        @*<a asp-controller="Route" asp-action="RoutePage" asp-route-id="@item.IdRout" class="boxed-btn3">Посмотреть</a>*@
                                     <a  class="boxed-btn3"  asp-action="UserPage" asp-controller="User" asp-route-id="@user.IdUsers"> Оставить отзыв</a>
                                </div>
                                @*<div class="date">
                                    <p>Дата размещениия:</p>
                                </div>*@
                            </div>
                        </div>
                    </div>
                }

                @foreach (DiplomProba1.Models.Data.Userroute item in @ViewBag.FutereRoute) {
                    <div class="col-lg-12 col-md-12">
                        
                        <div class="single_jobs white-bg d-flex justify-content-between">
                            
                            <div class="jobs_left d-flex align-items-center">
                                <div class="thumb ">
                                    <img class="DriverPh" src="@item.IdRoutNavigation.IdUserNavigation.PhotoUser" alt="Profile Picture">
                                </div>
                                <div class="jobs_conetent">
                                    
                                    <a  asp-action="UserPage" asp-controller="User" asp-route-id="@item.IdUser"><h4>Водитель: @item.IdRoutNavigation.IdUserNavigation.Fulname</h4></a>
                                    <div class="flexwrap links_locat d-flex align-items-center">
                                        <div class="location">
                                            <p> <i class="fa fa-map-marker"></i>Из: @item.IdRoutNavigation.BeginRoute </p>
                                            <p> <i class="fa fa-map-marker"></i>В: @item.IdRoutNavigation.EndRoute </p>
                                        </div>
                                        <div class="location">
                                            <p> <i class="fa fa-clock-o"></i> Дата: @item.IdRoutNavigation.DataRoute</p>
                                            <p> <i class="fa fa-clock-o"></i> Время: @item.IdRoutNavigation.TimeRoute</p>
                                        </div>
                                        <div class="location">
                                            <p> <i class="fa fa-clock-o"></i> Забронировано мест: @item.Bookcount</p>
                                            <p> <i class="fa fa-clock-o"></i> Цена места: @item.IdRoutNavigation.Cost ₽</p>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="jobs_right myflex-column">
                                <div class="apply_now">
                                    @*<a class="heart_mark" href="#"> <i class="ti-heart">❤</i> </a>*@
                                    <a asp-controller="Route" asp-action="RoutePage" asp-route-id="@item.IdRout" class="boxed-btn3">Посмотреть</a>
                                </div>
                                <div class="date">
                                    <p>Дата размещениия: @item.IdRoutNavigation.DateAddedRoude</p>
                                </div>
                            </div>
                        </div>
                    </div>
                }

                
                </div>
            </div>
        </div>
    </div>


        
   
   
    




<!-- Модальное окно -->


</div>
